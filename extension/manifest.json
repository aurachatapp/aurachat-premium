{
  "manifest_version": 3,
  "name": "AuraChat",
  "version": "1.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": { "128": "icons/icon128.png" }
  },
  "permissions": ["storage"],
  "host_permissions": ["https://aurachatapp.github.io/aurachat-premium/*"],
  ,
  "content_scripts": [
    {
      "matches": ["https://aurachatapp.github.io/aurachat-premium/*"],
      "js": ["content.session.js"],
      "run_at": "document_start"
    }
  ]
}